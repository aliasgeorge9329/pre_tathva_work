<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" />
    <title>Tathva</title>
    <meta charset="utf-8" />
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
    />
    <link rel="stylesheet" href="/css/styles.css" />
    <nav class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <p class="navbar-brand">TATHVA</p>
        </div>
        <ul class="nav navbar-nav navbar-right">
          <li id="register"><a href="/register">REGISTER</a></li>
          <li id="login"><a href="/login">LOGIN</a></li>
        </ul>
      </div>
    </nav>

    <body>
      <div class="container">



<h1>Accepted Sentence</h1>
<br>
<% sentence.forEach(function(user_data){ if (user_data.sentence != null && user_data.status == "accepted") {%>
           
<li style="font-size: 2.5rem;"><%=user_data.sentence%> - <%=user_data.name%>, <%=user_data.college%></li> 
<br>

<% };}) %>


</div>
<div class="footer-padding"></div>
  <div class="footer">
  </div>
</div>
</body>
